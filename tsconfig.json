{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
    "build": "tsc"
    "start": "node dist/server.js"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]

}
